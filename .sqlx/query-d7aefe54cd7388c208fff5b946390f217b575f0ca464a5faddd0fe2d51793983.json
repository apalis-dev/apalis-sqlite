{
  "db_name": "SQLite",
  "query": "UPDATE Jobs\nSET\n    status = 'Queued',\n    lock_by = ?1,\n    lock_at = strftime('%s', 'now')\nWHERE\n    ROWID IN (\n        SELECT ROWID\n        FROM Jobs\n        WHERE job_type = ?2\n            AND (\n                    (status = 'Pending' AND lock_by IS NULL) \n                    OR \n                    (status = 'Failed' AND attempts < max_attempts)\n                )\n            AND (run_at IS NULL OR run_at <= strftime('%s', 'now'))\n        ORDER BY priority DESC, run_at ASC, id ASC\n        LIMIT ?3\n    )\nRETURNING *\n",
