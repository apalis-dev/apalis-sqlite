{
  "db_name": "SQLite",
  "query": "UPDATE Jobs\nSET\n    status = 'Queued',\n    lock_at = strftime('%s', 'now')\nWHERE ROWID IN (\n    SELECT ROWID\n    FROM Jobs\n    WHERE job_type IN (\n            SELECT value FROM json_each(?1)\n        )\n        AND status = 'Pending'\n        AND lock_by IS NULL\n        AND (\n            run_at IS NULL\n            OR run_at <= strftime('%s', 'now')\n        )\n        AND ROWID IN (\n            SELECT value FROM json_each(?2)\n        )\n    ORDER BY\n        priority DESC,\n        run_at ASC,\n        id ASC\n    LIMIT ?3\n)\nRETURNING *;\n",
